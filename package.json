{
  "name": "framework-cli",
  "version": "0.0.1",
  "description": "Gluestack V2 Framework CLI Project",
  "main": "index",
  "scripts": {
    "install:all": "npm install --workspaces --if-present",
    "build:plugins": "npm run build --workspaces --if-present",
    "lint:plugins": "npm run lint --workspaces --if-present",
    "watch:cli": "npm run watch --workspace @gluestack-v2/framework-cli",
    "watch:develop": "npm run watch --workspace @gluestack-v2/glue-plugin-develop",
    "watch:web": "npm run watch --workspace @gluestack-v2/glue-plugin-web",
    "watch:sdk": "npm run watch --workspace @gluestack-v2/glue-plugin-service-sdk",
    "watch:gateway": "npm run watch --workspace @gluestack-v2/glue-plugin-service-gateway",
    "watch:functions": "npm run watch --workspace @gluestack-v2/glue-plugin-functions",
    "watch:router-nginx": "npm run watch --workspace @gluestack-v2/glue-plugin-router-nginx",
    "watch:components": "npm run watch --workspace @gluestack-v2/glue-plugin-components",
    "watch:cron": "npm run watch --workspace @gluestack-v2/glue-plugin-cron",
    "watch:openai": "npm run watch --workspace @gluestack-v2/glue-plugin-openai",
    "watch:events": "npm run watch --workspace @gluestack-v2/glue-plugin-events",
    "add:necessory": "node glue add @gluestack-v2/glue-plugin-develop develop && node glue add @gluestack-v2/glue-plugin-web web && node glue add @gluestack-v2/glue-plugin-service-sdk sdk && node glue add @gluestack-v2/glue-plugin-service-gateway gateway && node glue add @gluestack-v2/glue-plugin-functions functions && node glue add @gluestack-v2/glue-plugin-events events",
    "watch:all": "npm run watch:cli & npm run watch:develop & npm run watch:gateway & npm run watch:sdk & npm run watch:functions & npm run watch:web & npm run watch:events",
    "clean": "echo 'hello'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gluestack-v2/glue-cli"
  },
  "keywords": [
    "gluestack",
    "v2",
    "fullstack",
    "framework",
    "cli",
    "mobile",
    "web",
    "storybook",
    "shared-components"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.5"
  },
  "workspaces": [
    "packages/*",
    "/Users/suraj/Sites/projects/gluestack-v2/framework-cli/web",
    "/Users/suraj/Sites/projects/gluestack-v2/framework-cli/functions",
    ".glue/__generated__/packages",
    "/Users/suraj/Sites/projects/gluestack-v2/framework-cli/.glue/__generated__/seal/services/gateway/src/gateway",
    "/Users/suraj/Sites/projects/gluestack-v2/framework-cli/.glue/__generated__/packages/sdk/src/sdk"
