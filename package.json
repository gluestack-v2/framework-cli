{
  "name": "framework-cli",
  "version": "0.0.1",
  "description": "Gluestack V2 Framework CLI Project",
  "main": "index",
  "scripts": {
    "install:all": "npm install --workspaces --if-present",
    "build:plugins": "npm run build --workspaces --if-present",
    "lint:plugins": "npm run lint --workspaces --if-present",
    "watch:cli": "npm run watch --workspace @gluestack-v2/framework-cli",
    "watch:develop": "npm run watch --workspace @gluestack-v2/glue-plugin-develop",
    "watch:web": "npm run watch --workspace @gluestack-v2/glue-plugin-web",
    "watch:sdk": "npm run watch --workspace @gluestack-v2/glue-plugin-service-sdk",
    "watch:gateway": "npm run watch --workspace @gluestack-v2/glue-plugin-service-gateway",
    "watch:functions": "npm run watch --workspace @gluestack-v2/glue-plugin-functions",
    "watch:router-nginx": "npm run watch --workspace @gluestack-v2/glue-plugin-router-nginx",
    "watch:components": "npm run watch --workspace @gluestack-v2/glue-plugin-components",
    "watch:crons": "npm run watch --workspace @gluestack-v2/glue-plugin-crons",
    "watch:openai": "npm run watch --workspace @gluestack-v2/glue-plugin-openai",
    "watch:events": "npm run watch --workspace @gluestack-v2/glue-plugin-events",
    "watch:middlewares": "npm run watch --workspace @gluestack-v2/glue-plugin-middlewares",
    "watch:database": "npm run watch --workspace @gluestack-v2/glue-plugin-database",
    "watch:graphql": "npm run watch --workspace @gluestack-v2/glue-plugin-graphql",
    "watch:queues": "npm run watch --workspace @gluestack-v2/glue-plugin-queues",
    "add:necessory": "node glue add @gluestack-v2/glue-plugin-develop develop && node glue add @gluestack-v2/glue-plugin-web web && node glue add @gluestack-v2/glue-plugin-service-sdk sdk && node glue add @gluestack-v2/glue-plugin-service-gateway gateway && node glue add @gluestack-v2/glue-plugin-functions functions && node glue add @gluestack-v2/glue-plugin-middlewares middlewares  && node glue add @gluestack-v2/glue-plugin-queues queues && node glue add @gluestack-v2/glue-plugin-crons crons && node glue add @gluestack-v2/glue-plugin-events events",
    "watch:all": "npm run watch:cli  & npm run watch:develop & npm run watch:gateway & npm run watch:crons & npm run watch:sdk & npm run watch:functions & npm run watch:web & npm run watch:events & npm run watch:queues & npm run watch:middlewares",
    "clean": "echo 'hello'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gluestack-v2/glue-cli"
  },
  "keywords": [
    "gluestack",
    "v2",
    "fullstack",
    "framework",
    "cli",
    "mobile",
    "web",
    "storybook",
    "shared-components"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.5"
  },
  "workspaces": [
    "packages/*",
    "web",
    ".glue/__generated__/seal/services/web/src/web",
    "/Users/rohitsingh/Documents/projects/gluestack/framework-cli/functions",
    ".glue/__generated__/packages",
    "node_modules/@gluestack-v2/glue-plugin-service-gateway/template",
    ".glue/__generated__/seal/services/gateway/src/gateway",
    "node_modules/@gluestack-v2/glue-plugin-service-sdk/template",
    ".glue/__generated__/packages/sdk"
  ],
  "dependencies": {
    "@gluestack-v2/glue-plugin-crons": "^0.0.1",
    "@gluestack-v2/glue-plugin-develop": "^0.0.2",
    "@gluestack-v2/glue-plugin-events": "^0.0.2",
    "@gluestack-v2/glue-plugin-functions": "^0.0.1",
    "@gluestack-v2/glue-plugin-middlewares": "^0.0.2",
    "@gluestack-v2/glue-plugin-queues": "^0.0.1",
    "@gluestack-v2/glue-plugin-service-gateway": "^0.0.1",
    "@gluestack-v2/glue-plugin-service-sdk": "^0.0.2",
    "@gluestack-v2/glue-plugin-web": "^0.0.2"
  }
}
